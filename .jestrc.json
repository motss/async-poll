{
  "globals": {
  },
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "collectCoverageFrom": [
    "!**/node_modules/**",
    "!**/dist/demo/**/*.{js|jsx}",
    "!**/dist/test*/**/*.{js|jsx}",
    "!**/dist/*_typings.js",
    "**/dist/**/*.{js|jsx}"
  ],
  "verbose": true,
  "bail": true,
  "roots": [
    "<rootDir>/dist/"
  ],
  "testMatch": [
    "**/dist/test*/**/*.spec.(js|jsx)"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ]
}
